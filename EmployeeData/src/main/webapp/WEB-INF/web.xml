<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetEmployee</servlet-name>
  	<display-name>GetEmployee</display-name>
  	<description></description>
  	<servlet-class>GetEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConvertEmployee</servlet-name>
  	<display-name>ConvertEmployee</display-name>
  	<description></description>
  	<servlet-class>ConvertEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetEmployee</servlet-name>
  	<url-pattern>/GetEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConvertEmployee</servlet-name>
  	<url-pattern>/ConvertEmployee</url-pattern>
  </servlet-mapping>
</web-app>
